    "@types/js-search": "^1.4.4",
    "@types/node": "^22.5.4",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/react-test-renderer": "^18.3.0",
    "@types/sass": "^1.45.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
