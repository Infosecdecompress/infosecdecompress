    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "gh-pages": "^6.1.1",
    "identity-obj-proxy": "3.0.0",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
