{
  "name": "infosecdecompress",
  "version": "2.5.0",
  "description": "資安解壓縮的家 Website of Infosec Decompress",
  "keywords": [
    "gatsby",
    "security",
    "informationsecurity",
    "cybersecurity",
    "sentryio",
    "sentry"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Infosecdecompress/infosecdecompress"
  },
  "license": "MIT",
  "author": "Jeffrey Hung <jeff@infosecdecompress.com>",
  "scripts": {
    "build": "npm run clean && gatsby build",
    "clean": "rimraf .cache public",
    "cleandeploy": "npm run clean && gatsby build",
    "deploy": " gatsby build && gh-pages -d public -b gh-pages",
    "develop": "npm run clean && gatsby develop",
    "format": "npm run format:ts && npm run format:scss",
    "format:scss": "stylelint \"src/**/*.scss\" --fix",
    "format:ts": "eslint \"src\" --ext .tsx,.ts --fix && prettier --write .",
    "lint": "npm run lint:ts && npm run lint:scss",
    "lint:commit": "commitlint --edit",
    "lint:scss": "stylelint \"src/**/*.scss\"",
    "lint:staged": "lint-staged",
    "lint:ts": "eslint \"src\" --ext .tsx,.ts && prettier --check .",
    "serve": "gatsby serve",
    "start": "npm run clean && gatsby develop",
    "test": "jest --config ./internal/testing/jest-config.ts",
    "test:coverage": "jest --coverage --config ./internal/testing/jest-config.ts",
    "test:watch": "jest --watch --config ./internal/testing/jest-config.ts"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{tsx?,ts?}": [
      "npm run format:ts"
    ],
    "*.{css?,scss?}": [
      "npm run format:scss"
    ]
  },
  "dependencies": {
    "@sentry/gatsby": "^7.2.0",
    "@sentry/react": "^7.2.0",
    "@sentry/tracing": "^7.2.0",
    "axios": "^0.27.2",
    "classnames": "^2.3.1",
    "disqus-react": "^1.1.3",
    "gatsby": "^4.16.0",
    "gatsby-link": "^4.16.0",
    "gatsby-plugin-catch-links": "^4.16.0",
    "gatsby-plugin-csp": "^1.1.3",
    "gatsby-plugin-feed": "^4.16.0",
    "gatsby-plugin-google-gtag": "^4.16.0",
    "gatsby-plugin-image": "^2.16.1",
    "gatsby-plugin-json-output": "^1.2.0",
    "gatsby-plugin-manifest": "^4.16.0",
    "gatsby-plugin-offline": "^5.16.0",
    "gatsby-plugin-optimize-svgs": "^1.0.5",
    "gatsby-plugin-react-helmet": "^5.16.0",
    "gatsby-plugin-sass": "^5.16.0",
    "gatsby-plugin-security-txt": "^1.1.4",
    "gatsby-plugin-sharp": "^4.16.1",
    "gatsby-plugin-sitemap": "^5.16.0",
    "gatsby-plugin-sri": "^1.1.0",
    "gatsby-remark-autolink-headers": "^5.16.0",
    "gatsby-remark-copy-linked-files": "^5.16.0",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-images": "^6.16.0",
    "gatsby-remark-prismjs": "^6.16.0",
    "gatsby-remark-responsive-iframe": "^5.16.0",
    "gatsby-remark-smartypants": "^5.16.0",
    "gatsby-source-filesystem": "^4.16.0",
    "gatsby-transformer-remark": "^5.16.0",
    "gatsby-transformer-sharp": "^4.16.0",
    "js-search": "^2.0.0",
    "prismjs": "^1.28.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "@alxshelepenok/eslint-config": "^1.0.64",
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@jest/globals": "^27.5.1",
    "@swc/core": "^1.2.203",
    "@swc/jest": "^0.2.21",
    "@types/gatsby-transformer-remark": "^2.9.1",
    "@types/jest": "^27.5.2",
    "@types/js-search": "^1.4.0",
    "@types/node": "^17.0.24",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.5",
    "@types/react-helmet": "^6.1.5",
    "@types/react-test-renderer": "^18.0.0",
    "@types/sass": "^1.43.1",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "autoprefixer": "^10.4.7",
    "browserslist": "^4.20.3",
    "codecov": "^3.8.3",
    "concurrently": "^7.2.1",
    "conventional-changelog-conventionalcommits": "5.0.0",
    "eslint": "^8.17.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.5.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "gh-pages": "^4.0.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "^27.5.1",
    "jest-cli": "^27.5.1",
    "jest-environment-jsdom": "^27.5.1",
    "jest-svg-transformer": "^1.0.0",
    "lint-staged": "^12.5.0",
    "lost": "8.3.1",
    "postcss": "^8.4.14",
    "postcss-scss": "^4.0.4",
    "prettier": "^2.6.2",
    "prettier-plugin-packagejson": "^2.2.18",
    "react-test-renderer": "^18.1.0",
    "rimraf": "3.0.2",
    "sass": "^1.52.3",
    "source-map-support": "^0.5.21",
    "stylelint": "^14.9.1",
    "stylelint-config-recommended-scss": "^6.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.3.0",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3"
  },
  "engines": {
    "node": ">=v16.15.1"
  }
}
