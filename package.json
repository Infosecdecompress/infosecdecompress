{
  "name": "infosecdecompress",
  "version": "4.0.0",
  "description": "資安解壓縮的家 Website of Infosec Decompress",
  "keywords": [
    "gatsby",
    "security",
    "informationsecurity",
    "cybersecurity",
    "sentryio",
    "sentry"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Infosecdecompress/infosecdecompress"
  },
  "license": "MIT",
  "author": "Jeffrey Hung <jeff@infosecdecompress.com>",
  "scripts": {
    "build": "npm run clean && gatsby build",
    "clean": "rimraf .cache public",
    "cleandeploy": "npm run clean && gatsby build",
    "deploy": " gatsby build && gh-pages -d public -b gh-pages",
    "develop": "npm run clean && gatsby develop",
    "format": "npm run format:ts && npm run format:scss",
    "format:scss": "stylelint \"src/**/*.scss\" --fix",
    "format:ts": "eslint \"src\" --ext .tsx,.ts --fix && prettier --write .",
    "lint": "npm run lint:ts && npm run lint:scss",
    "lint:commit": "commitlint",
    "lint:scss": "stylelint \"src/**/*.scss\"",
    "lint:staged": "lint-staged",
    "lint:ts": "eslint \"src\" --ext .tsx,.ts && prettier --check .",
    "serve": "gatsby serve",
    "start": "npm run clean && gatsby develop",
    "test": "jest --config ./internal/testing/jest-config.ts",
    "test:update-snapshot": "jest --updateSnapshot --config ./internal/testing/jest-config.ts",
    "test:watch": "jest --watch --config ./internal/testing/jest-config.ts"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.tsx?": [
      "npm run format:ts"
    ],
    "*.(sc|sa|c)ss": [
      "npm run format:scss"
    ]
  },
  "dependencies": {
    "@alxshelepenok/diesel": "^1.0.1367",
    "@alxshelepenok/diesel-extensions": "^1.0.1242",
    "@sentry/gatsby": "^10.38.0",
    "@sentry/react": "^10.38.0",
    "@sentry/tracing": "^7.120.4",
    "abab": "^2.0.6",
    "axios": "^1.13.4",
    "classnames": "^2.5.1",
    "disqus-react": "^1.1.7",
    "gatsby": "^5.16.0",
    "gatsby-link": "^5.16.0",
    "gatsby-plugin-catch-links": "^5.16.0",
    "gatsby-plugin-csp": "^1.1.4",
    "gatsby-plugin-feed": "^5.16.0",
    "gatsby-plugin-image": "^3.16.0",
    "gatsby-plugin-json-output": "^1.2.0",
    "gatsby-plugin-manifest": "^5.16.0",
    "gatsby-plugin-offline": "^6.16.0",
    "gatsby-plugin-optimize-svgs": "^1.0.5",
    "gatsby-plugin-sass": "^6.16.0",
    "gatsby-plugin-security-txt": "^1.2.1",
    "gatsby-plugin-sharp": "^5.16.0",
    "gatsby-plugin-sitemap": "^6.16.0",
    "gatsby-plugin-sri": "^1.1.0",
    "gatsby-remark-autolink-headers": "^6.16.0",
    "gatsby-remark-copy-linked-files": "^6.16.0",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-images": "^7.16.0",
    "gatsby-remark-prismjs": "^7.16.0",
    "gatsby-remark-responsive-iframe": "^6.16.0",
    "gatsby-remark-smartypants": "^6.16.0",
    "gatsby-source-filesystem": "^5.16.0",
    "gatsby-transformer-remark": "^6.16.0",
    "gatsby-transformer-sharp": "^5.16.0",
    "js-search": "^2.0.1",
    "prismjs": "^1.30.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@alxshelepenok/eslint-config": "^1.0.599",
