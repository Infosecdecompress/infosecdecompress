    "katex": "^0.12.0",
    "lodash": "^4.17.20",
    "netlify-cms-app": "^2.12.27",
    "node-sass": "^4.14.1",
    "normalize-scss": "^7.0.1",
    "prismjs": "^1.22.0",
    "react": "^16.14.0",
