    "jest-svg-transformer": "^1.0.0",
    "lint-staged": "^15.2.10",
    "lost": "9.0.2",
    "postcss": "^8.4.41",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.3.3",
    "prettier-plugin-packagejson": "^2.5.2",
    "react-test-renderer": "^18.3.1",
    "rimraf": "6.0.1",
    "sass": "^1.78.0",
    "source-map-support": "^0.5.21",
    "stylelint": "^14.16.1",
    "stylelint-config-recommended-scss": "^8.0.0",
    "stylelint-order": "^6.0.4",
    "stylelint-scss": "^5.3.2",
