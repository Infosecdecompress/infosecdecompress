{
  "extends": ["config:base"],
  "packageRules": [
    {
      "packagePatterns": ["^stylelint"],
      "groupName": "stylelint"
    },
    {
      "packagePatterns": ["^eslint"],
      "groupName": "eslint"
    },
    {
      "packagePatterns": ["gatsby"],
      "groupName": "gatsby"
    },
    {
      "packageNames": ["react", "react-dom"],
      "groupName": "react"
    }
  ],
  "labels": ["renovate"],
  "prHourlyLimit": 4,
  "prConcurrentLimit": 16,
  "rangeStrategy": "bump",
  "semanticCommits": "enabled"
}
